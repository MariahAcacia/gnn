<%= render template: "shared/navbar" %>

<div class="row index-wrapper">
  <div class="col-sm-10 offset-sm-1">
    <h1>Your Saved Text Links: </h1>
  </div>
</div>

<div class="row">
  <div class="col-sm-10 offset-sm-1">
    <div class="row">
      <% @texts.each do |x| %>
        <%= render partial: "shared/text_box", locals: { article: x } %>
      <% end %>
    </div>
  </div>
</div>
